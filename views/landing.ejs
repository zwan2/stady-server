  <!-- S T A R T  D O W N L O A D -->
  <section id="download">
    <div class="container">
      <div class="row">

        <!-- Left Section -->
        <div class="col-md-6 mt-4">
          <div class="content-box">
            <span>Notice</span>
            <h3><%= notice %></h3>
            <p>
              <%= content %>
            </p>
          </div>
        </div>


        <!-- Right Section -->
        <div class="col-md-6 mt-4">
         
          <a href="<%= apkLink %>" id="downLink">   
            <div class="card pricing-box">
              <div class="card-body">
                <div class="float-left">
                  <span>
                    <%= versionStatus %></span><br>
                  <inline class="h2" style="color: #3a3f52;">
                    <%= versionReal %>
                  </inline>
                </div>
                <div class="float-right align-auto-center">
                  <i class="material-icons white-center-icon">get_app</i>
                </div>
              </div>
            </div>
          </a>

          
          
          <br> <br>

        <div class="changes">

          <ul>
              <%
                var sentences = changes.split("\r\n");
              %>
              <% for (var i=0 ; i<sentences.length ; i++) { %>
                  <% sentences[i] = sentences[i].replace("- ", ""); %>
                  <li>
                  
                    <i class="icon ion-md-checkmark-circle-outline float-left text-left"></i>
                  
                    <inline class=""><%= sentences[i] %></inline>
                  
                  </li> 
              <% } %>
            </ul>
          </div>
        </div>


          
